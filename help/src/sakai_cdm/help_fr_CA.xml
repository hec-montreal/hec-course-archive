<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

   <bean id="hec-course-archive-help" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>help_fr_ca</value></property>
      <property name="name"><value>Archive des plans de cours</value></property>
      <property name="location"><value>/sakai_cmd/help_fr_CA.html</value></property>
      <property name="defaultForTool"><value>sakai.hec-course-archive</value></property>
   </bean>

    <bean id="org.sakaiproject.api.app.help.TableOfContents" class="org.sakaiproject.component.app.help.model.TableOfContentsBean">
        <property name="name">
            <value>root</value>
        </property>
        <property name="categories">
            <list>
                <bean id="archiveCategory" class="org.sakaiproject.component.app.help.model.CategoryBean">
                    <property name="name">
                        <value>Archives HEC</value>
                    </property>
                    <property name="resources">
	                  <list>
	                     <ref bean="hec-course-archive-help"/>
	                  </list>
	               </property>
                </bean>
            </list>
        </property>
    </bean>
</beans>
