<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
  
    <bean id="org.sakaiproject.api.app.help.TableOfContents" class="org.sakaiproject.component.app.help.model.TableOfContentsBean">
        <property name="name">
            <value>root</value>
        </property>
        <property name="categories">
            <list>
                <bean id="archiveCategory" class="org.sakaiproject.component.app.help.model.CategoryBean">
                    <property name="name">
                        <value>HEC Archive</value>
                    </property>
                </bean>
            </list>
        </property>
    </bean>
</beans>